#Tugas praktek while for
list_cash_flow = [2500000, 5000000, -1000000, -2500000, 5000000, 10000000, -5000000, 7500000, 10000000, -1500000, 25000000, -2500000]
total_spanding, total_income = 0,0
for dana in list_cash_flow:
    if dana > 0:
        total_income += dana
    else:
        total_spanding += dana
    total_spanding *= -1
print(total_spanding)
print(total_income)
        